<?xml version = "1.0" encoding = "UTF-8" ?>
<Application xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:Local="clr-namespace:SysCredit"
             x:Class="SysCredit.App"
             RequestedThemeChanged="OnRequestedThemeChanged">
    <Application.Resources>
        <ResourceDictionary>
            <x:String x:Key="DevCompanyName">W&amp;A</x:String>
            <x:String x:Key="CompanyName">SysCredit</x:String>
            <x:String x:Key="AppName">SysCredit</x:String>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Resources/Styles/Colors.xaml" />
                <ResourceDictionary Source="Resources/Styles/Styles.xaml" />
                <ResourceDictionary Source="Resources/Styles/SysCredit.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
